app: outlook
-
new e-mail: key(cmd-n)
discard e-mail: key(cmd-shift-d)
reply: key(cmd-r)
reply all: key(cmd-shift-r)
send e-mail: key(cmd-enter)
mark read: key(cmd-t)
mark unread: key(cmd-shift-t)
archive e-mail: key(ctrl-e)
next pain: key(shift-ctrl-[)
previous pain: key(shift-ctrl-])

# navigation
calendar: key(cmd-2)
inbox: key(cmd-1)
